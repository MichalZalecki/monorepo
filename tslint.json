{
  "extends": "typestrict",
  "rules": {
    "no-console": false,
    "no-debugger": true,
    "typedef": [true, "call-signature"],
    "mocha-avoid-only": true,
    "interface-name": [true, "never-prefix"],
    "no-commented-code": true,
    "no-use-before-declare": false,
    "await-promise": [true, "RequestPromise", "Bluebird"]
  }
}
